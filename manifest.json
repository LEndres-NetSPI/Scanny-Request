{
  "manifest_version": 3,
  "name": "Scanny webRequest Alpha",
  "version": "1.0",
  "permissions": ["webRequest", "storage", "webNavigation"],
  "host_permissions": [
     "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
